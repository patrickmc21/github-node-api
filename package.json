{
  "name": "github-node-api",
  "version": "1.0.0",
  "description": "A NodeJS/TypeScript API to fetch and build data from the GithHub API",
  "main": "index.js",
  "repository": "git@github.com:patrickmc21/github-node-api.git",
  "author": "Patrick McLaughlin <patrickmc21@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "start": "ts-node-dev --respawn ./src/server.ts",
    "debug": "ts-node-dev --inspect --respawn ./src/server.ts",
    "lint": "mkdir -p build && tslint -c tslint.json -o build/tslint-out.json -t json 'src/**/*.ts'",
    "prettier": "prettier --write 'src/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.0",
    "swagger-jsdoc": "^6.2.7",
    "swagger-ui-express": "^4.6.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.17",
    "@types/node-fetch": "^2.6.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/webpack": "^5.28.0",
    "@types/winston": "^2.4.4",
    "@webpack-cli/generators": "^3.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "wepack-cli": "^0.0.1-security"
  }
}
