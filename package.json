{
  "name": "github-node-api",
  "version": "1.0.0",
  "description": "A NodeJS/TypeScript API to fetch and build data from the GithHub API",
  "main": "index.js",
  "repository": "git@github.com:patrickmc21/github-node-api.git",
  "author": "Patrick McLaughlin <patrickmc21@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev --respawn ./src/server.ts",
    "debug": "ts-node-dev --inspect --respawn ./src/server.ts",
    "lint": "mkdir -p build && tslint -c tslint.json -o build/tslint-out.json -t json 'src/**/*.ts'",
    "prettier": "prettier --write 'src/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.17",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.3.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.9.4"
  }
}
