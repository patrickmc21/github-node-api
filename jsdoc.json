{
  "source": {
    "include": ["./src"],
    "includePattern": ".ts$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown", "node_modules/better-docs/typescript"],
  "opts": {
    "encoding": "utf8",
    "readme": "./readme.md",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/better-docs"
  },
  "tags": {
    "allUnknownTags": ["optional"]
  }
}
